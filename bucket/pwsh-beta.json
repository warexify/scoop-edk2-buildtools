    "post_install": [
        "if (Test-Path(\"$dir\\pwsh-install-context.reg\")) {",
        "  $pwshpath = \"$dir\".Replace('\\', '\\\\')",
        "  $content = Get-Content \"$dir\\pwsh-install-context.reg\"",
        "  $content = $content.Replace('$pwsh', $pwshpath)",
        "  $content | Set-Content -Path \"$dir\\pwsh-install-context.reg\"",
        "}"
    ],
    "notes": "Add PowerShell as a context menu option by running: '$dir\\pwsh-install-context.reg'",
    "checkver": {
        "url": "https://github.com/PowerShell/PowerShell/releases.atom",
