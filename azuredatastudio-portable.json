{
    "version": "1.7.0",
    "description": "Data management tool that enables work with SQL Server, Azure SQL DB and SQL DW.",
    "homepage": "https://docs.microsoft.com/en-us/sql/azure-data-studio",
    "license": "MIT",
    "url": "https://github.com/Microsoft/azuredatastudio/releases/download/1.7.0/azuredatastudio-windows-1.7.0.zip",
    "hash": "98ebd0f05ebafcc7165b7acd71999e09a1ccc3102922a3470dc71ca0f2a85ad6",
    "notes": [
        "Azure Data Studio now supports Portable Mode! Please move the following directories:",
        "From \"$env:USERPROFILE\\.azuredatastudio\\extensions\" to \"$env:ALLUSERPROFILE\\scoop\\persist\\azuredatastudio-portable\\data\\extensions\"",
        "From \"$env:APPDATA\\azuredatastudio\" to \"$env:ALLUSERPROFILE\\scoop\\persist\\azuredatastudio-portable\\data\\user-data\""
    ],
    "extract_dir": "azuredatastudio-windows",
    "persist": "data",
    "bin": [
        [
            "bin\\azuredatastudio.cmd",
            "azuredatastudio",
            "--user-data-dir $persist_dir\\data\\user-data --extensions-dir $persist_dir\\data\\extensions"
        ]
    ],
    "shortcuts": [
        [
            "azuredatastudio.exe",
            "Azure Data Studio",
            "--user-data-dir $persist_dir\\data\\user-data --extensions-dir $persist_dir\\data\\extensions"
        ]
    ],
    "checkver": {
        "github": "https://github.com/Microsoft/azuredatastudio/"
    },
    "autoupdate": {
        "url": "https://github.com/Microsoft/azuredatastudio/releases/download/$version/azuredatastudio-windows-$version.zip"
    }
}
